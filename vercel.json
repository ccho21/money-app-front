{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://money-map.fly.dev/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://money-app-front-ecru.vercel.app"
        }
      ]
    }
  ]
}
