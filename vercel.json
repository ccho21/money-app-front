{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://localhost:8080/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "http://localhost:8080"
        }
      ]
    }
  ]
}
