  "/dashboard/calendar": {
    "entryPoint": "src/app/dashboard/calendar/page.tsx",
    "purpose": "월간 달력 뷰를 통해 일별 소비/수입을 시각적으로 보여주는 화면",
    "entryType": "dashboard",
    "designSpec": {
      "layout": "전체 페이지에 달력 UI, 날짜별 소형 수입/지출 요약 표시",
      "components": [
        {
          "name": "CalendarView",
          "props": {
            "calendarData": "TransactionCalendarDTO[]",
            "onDateClick": "(date: string) => void"
          }
        }
      ],
      "interaction": [
        "날짜 클릭 시 해당 일자 상세 이동",
        "달력 상호작용으로 월 전환"
      ],
      "wireframe": "structure.frontend.routes.dashboard-calendar.png"
    },
    "criticalData": ["TransactionCalendarDTO"],
    "stores": ["useTransactionStore", "useFilterStore"],
    "features": ["getTransactionCalendarView"],
    "components": ["CalendarView", "CalendarGrid", "CalendarCell"],
    "atomicComponents": ["Card", "Badge", "Skeleton"],
    "roleComponents": ["CalendarView"]
  },
  "/dashboard/daily": {
    "entryPoint": "src/app/dashboard/daily/page.tsx",
    "module": "dashboard",
    "entryType": "dashboard",
    "purpose": "일별 거래 요약 및 카테고리 기반 분류",
    "uses": {
      "store": ["src/modules/transaction/store.ts"],
      "formStore": ["src/modules/transaction/formStore.ts"],
      "api": ["src/modules/stats/api.ts"],
      "hooks": ["src/modules/stats/hooks.ts"],
      "types": ["src/modules/stats/types.ts"]
    },
    "dto": {
      "request": "StatsQuery",
      "response": "TransactionGroupSummaryDTO",
      "fromModule": "stats"
    },
    "components": [
      "DailyView",
      "SummaryBox",
      "TransactionGroup",
      "TransactionItem",
      "DateNavigator"
    ],
    "componentProps": {
      "DailyView": {
        "isLoading": "boolean",
        "data": "TransactionGroupSummaryDTO | null",
        "summaryItems": "SummaryItem[]",
        "onTransactionClick": "(tx: TransactionDetailDTO) => void",
        "onHeaderClick": "(date: string) => void"
      }
    },
    "designSpec": {
      "layout": "Top summary cards followed by grouped transaction list",
      "interaction": [
        "Group header click navigates to /stats/category/[id]",
        "Transaction item click opens transaction detail modal"
      ],
      "components": [
        {
          "name": "DateNavigator",
          "props": {
            "date": "string",
            "onChange": "(date: string) => void"
          }
        },
        {
          "name": "SummaryBox",
          "props": {
            "items": "SummaryItem[]"
          }
        },
        {
          "name": "TransactionGroup",
          "props": {
            "data": "TransactionGroupSummaryDTO"
          }
        }
      ],
      "wireframe": "structure.frontend.routes.dashboard-daily.png"
    },
    "style": {
      "theme": "flat, minimalist, custom-tailored",
      "goal": "Move away from legacy finance app look; emphasize whitespace and clean flow.",
      "colors": {
        "background": "#F8F9FA",
        "card": "#FFFFFF",
        "textPrimary": "#111827",
        "textMuted": "#6B7280",
        "income": "#16A34A",
        "expense": "#EF4444",
        "accent": "#3B82F6"
      },
      "designNotes": [
        "Rounded cards with large touch targets",
        "Light shadows for element grouping",
        "Text-based hierarchy over graphical emphasis",
        "Avoid heavy borders and gradients",
        "Use lucide-react icons with consistent spacing"
      ]
    },
    "dataFlow": [
      "DailyPage → fetchTransactionSummary() → useTransactionStore.transactionSummaryResponse",
      "→ DailyView(data: TransactionGroupSummaryDTO)",
      "→ TransactionGroup(group: TransactionGroupItemDTO)",
      "→ TransactionItem(tx: TransactionDetailDTO)"
    ],
    "storeContracts": {
      "useTransactionStore": {
        "state": [
          "transactions",
          "selectedTransaction",
          "transactionSummaryResponse",
          "transactionSummary",
          "transactionCalendarItems",
          "filters",
          "isLoading",
          "lastUpdatedAt",
          "error"
        ],
        "actions": [
          "setTransactions",
          "setSelectedTransaction",
          "setTransactionSummary",
          "setTransactionSummaryResponse",
          "setCalendarItems",
          "setFilters",
          "resetFilters",
          "setDateRange",
          "setLoading",
          "setError",
          "clear"
        ]
      },
      "useFilterStore": {
        "state": ["query"],
        "actions": ["getDateRangeKey", "setQuery"]
      }
    },
    "featureContracts": {
      "fetchTransactionSummaryAPI": {
        "input": "StatsQuery",
        "output": "TransactionGroupSummaryDTO"
      },
      "fetchAndSetTransactionSummary": {
        "wraps": "fetchTransactionSummaryAPI",
        "sets": "useTransactionStore.transactionSummaryResponse"
      }
    }
  },
  "/dashboard/monthly": {
    "entryPoint": "src/app/dashboard/monthly/page.tsx",
    "purpose": "월별 총 소비 및 수입을 요약 카드 + 카테고리별 리스트로 보여주는 화면",
