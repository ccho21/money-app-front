{
  "module": "stats",
  "entryPoints": [
    "app/stats/category/page.tsx",
    "app/stats/category/[id]/page.tsx",
    "app/stats/budget/page.tsx",
    "app/stats/budget/[id]/page.tsx",
    "app/stats/note/page.tsx",
    "app/stats/note/[note]/page.tsx"
  ],
  "routes": [
    "/stats/category",
    "/stats/category/[id]",
    "/stats/budget",
    "/stats/budget/[id]",
    "/stats/note",
    "/stats/note/[note]"
  ],
  "stores": ["modules/stats/store.ts"],
  "features": [
    "modules/stats/api.ts",
    "modules/stats/hooks.ts",
    "modules/stats/types.ts"
  ],
  "api": {
    "fetchStatByCategoryAPI": {
      "function": "fetchStatByCategoryAPI",
      "method": "GET",
      "url": "/stats/by-category",
      "responseDTO": "BaseListSummaryResponseDTO<StatsCategoryGroupItemDTO>",
      "description": "카테고리 기준 통계 그룹 조회",
      "usedBy": ["hooks.ts → fetchStatsByCategory"],
      "storeStrategy": {
        "store": "useStatsStore.ts",
        "stateShape": {
          "categoryResponse": "BaseListSummaryResponseDTO<StatsCategoryGroupItemDTO>"
        }
      }
    },
    "fetchStatsCategorySummaryAPI": {
      "function": "fetchStatsCategorySummaryAPI",
      "method": "GET",
      "url": "/stats/category/summary",
      "responseDTO": "StatsCategorySummaryDTO",
      "description": "카테고리 요약 통계 조회",
      "usedBy": ["hooks.ts → fetchStatsCategorySummary"],
      "storeStrategy": {
        "store": "useStatsStore.ts",
        "stateShape": {
          "categorySummaryResponse": "StatsCategorySummaryDTO"
        }
      }
    },
    "fetchStatsCategoryDetailAPI": {
      "function": "fetchStatsCategoryDetailAPI",
      "method": "GET",
      "url": "/stats/category/:id",
      "responseDTO": "StatsCategoryDetailDTO",
      "description": "카테고리 상세 통계 조회",
      "usedBy": ["hooks.ts → fetchStatsCategoryDetail"],
      "storeStrategy": {
        "store": "useStatsStore.ts",
        "stateShape": {
          "categoryDetailResponse": "StatsCategoryDetailDTO"
        }
      }
    }
  },
